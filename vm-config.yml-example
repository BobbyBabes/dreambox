name: 'dreambox'
php: '7'
ssl: true

sites:
  'dreambox':
    user: 'db_user' # required
    root: 'dreambox.com' # required
    public: 'html'
    local_root: 'web/' # required
    host: 'www.dreambox.test' # required
    aliases:
      - 'dreambox.test'
      - '*.dreambox.test'
    subdomains:
      app: 'app/'
      help: 'app/help/'
